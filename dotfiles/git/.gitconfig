# Shared Git configuration (team-safe)
# NOTE:
# - Do NOT set user.name / user.email here.
# - Put them in ~/.config/dotfiles/gitconfig.local instead.

[core]
  editor = vim
  excludesfile = ~/.gitignore_global

[alias]
  st = status
  co = checkout
  br = branch
  ci = commit

[init]
  defaultBranch = main

[pull]
  rebase = false

[include]
  path = ~/.config/dotfiles/gitconfig.local
